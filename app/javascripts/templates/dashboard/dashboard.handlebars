<div class="row">
  
  <div id="sidebar" class="span3">
    {{view Radium.ProfileView}}
    {{view Radium.DashboardFeedFilterView}}
    <h3>Team</h3>
    {{view Radium.UsersListView 
      contentBinding="Radium.usersController.content"
      selectedUserBinding="Radium.selectedUserFeedController.selectedUser"
    }}
  </div>
  
  <div class="span9">
    <div class="row">
        {{view Radium.GlobalSearchTextView}}
    </div>
    {{view Radium.AnnouncementsView}}
    <div id="form-container"></div>
        
    <div class="row">
      {{#with Radium.feedByKindController}}
        {{#if group}}
          {{#each group}}
            {{key}}, {{value}}<br>
          {{/each}}
        {{else}}
          loading...
        {{/if}}
      {{/with}}
    </div>
  </div>
</div>