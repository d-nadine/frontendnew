<i class="pull-left activity-icon icon-assigned"></i>
Assigned: 
  {{#if isCall}}
    {{#with view.resource.reference.contact}}
      <i class="icon-call"></i>
      Call <a href="/contacts/{{unbound id}}">{{unbound name}}</a>
    {{/with}}
  {{else}}
     <i class="icon-todo"></i>
    {{unbound reference.todo.description}}
  {{/if}}
{{#if view.resource.user}}
 (<a href="/users/{{unbound view.resource.user.id}}">{{unbound view.resource.user.abbrName}}</a>)
{{/if}}