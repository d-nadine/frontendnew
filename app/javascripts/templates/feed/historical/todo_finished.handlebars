{{#with todo}}
  {{#if isCall}}
    <i class="pull-left activity-icon icon-call"></i>
  {{else}}
    <i class="pull-left activity-icon icon-todo"></i>
  {{/if}}
  Finished:
    {{#if isCall}}
      {{#with contact}}
        <i class="icon-call"></i>
        Call <a {{bindAttr href="contact.url"}}>{{contact.name}}</a>
      {{/with}}
    {{else}}
       <i class="icon-todo"></i>
      {{description}}
    {{/if}}

  {{#if user}}
   (<a {{bindAttr href="user.url"}}>{{user.abbrName}}</a>)
  {{/if}}
{{/with}}