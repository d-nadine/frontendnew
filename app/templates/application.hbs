<div id="main" class="container">
  {{outlet topbar}}
  <div class="row">
    <div id="sidebar" class="span3">
      {{outlet sidebartoolbar}}
      <div id="scrollarea">
        <div class="viewport">
          <div class="scrollcontainer">
            <div id="scroller" class="scrollbar">
              <div class="track">
                <div class="thumb">
                  <div class="end"></div>
                </div>
              </div>
            </div>
          </div>
          <div class="overview">
            {{outlet sidebar}}
          </div>
        </div>
      </div>
    </div>
    <div id="main-panel" class="offset3 span9">
      <div class="row">
        <div class="span9">
          <div id="alerts"></div>
          {{outlet form}}

          {{outlet}}
        </div>
      </div>
    </div>
  </div>
</div>
