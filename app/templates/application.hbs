<div id="main" class="container">
  {{outlet topbar}}

  <div class="row">
    {{outlet notifications}}
    <div id="sidebar" class="span3">
      <div class="viewport">
        <div class="overview">
          {{outlet sidebar}}
        </div>
      </div>
    </div>

    <div id="main-panel" class="span9 offset3">
      {{view Radium.FormContainerView}}

      <div class="row">
        <div class="span9">
          <div id="alerts"></div>

          {{outlet}}
        </div>
      </div>
    </div>
  </div>
</div>
