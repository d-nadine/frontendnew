<div class="members">
  {{#if contacts.length}}
    <a href="#" {{action showMembers this}}>{{membersText}}</a>
    {{#each truncatedContacts}}
      <div class="contact">
        {{avatar this size=small class="img-polaroid"}}
        {{name}}
      </div>
    {{/each}}
    {{#if hasMoreContacts}}
      <strong><a href="#" {{action showMembers this}}>...</a></strong>
    {{/if}}
  {{else}}
    <label> No Members</label>
  {{/if}}
</div>
