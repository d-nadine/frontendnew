<div class="span1 checkers">
  {{check-box checked=isChecked class="checker-block"}}
</div>
<div class="span4 name">
  <div class="pull-left">
    {{avatar this size=medium class="img-polaroid"}}
  </div>
  <div class="clearfix search-list-item-title">
    <h4>
      {{#linkTo "contact" this}}
        {{displayName}}
      {{/linkTo}}
    </h4>
    {{partial 'search/links'}}
    <div class="title muted">{{title}}</div>
    {{#if company}}
      <div class="company">
        {{#linkTo "company" company}}{{company.name}}{{/linkTo}}
      </div>
    {{/if}}
  </div>
</div>
<div class="span2 last-activity">
  {{partial 'search/last_activity'}}
</div>
<div class="span2 next-task">
  {{partial 'search/tasks'}}
</div>
<div class="span3 groups">
  <div class="pull-right">
    {{partial 'search/delete'}}
  </div>
</div>