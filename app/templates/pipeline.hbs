<div id="widget-block" class="block">
  <ul class="breadcrumb" id="pipeline-widget">
    {{#each workflowGroups}}
      <li class="workflow">
        <a {{action selectGroup this}} href="#">{{title}}</a> {{pipelineBadge this}}
        <span class="divider">&gt;</span>
      </li>
    {{/each}}
    <li class="unpublished">
      {{#linkTo pipeline.unpublished}}Unpublished{{/linkTo}} {{badge unpublished.length style="info"}}
    </li>
  </ul>
</div>

<div id="pipeline-results" class="block">
  {{outlet confirmation}}
  {{outlet}}
</div>
