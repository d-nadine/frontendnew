<div class="pagination pull-right">
  <ul>
    <li><a href="#">«</a></li>
    <li><a href="#">1</a></li>
    <li><a href="#">»</a></li>
  </ul>
</div>
<table class="table table-striped table-bordered table-hover">
  <thead>
  <tr>
    <th>&nbsp;</th>
    <th>Lead</th>
    <th>Assigned To</th>
    <th>Age of Lead</th>
    <th>Next Task</th>
    <th>&nbsp;</th>
  </tr>
  </thead>
  <tbody>
    {{#each lead in view.content}}
      {{#with lead}}
        <tr>
          <td>
            {{view view.checkMailItem}}
          </td>
          <td>
            {{avatarImg lead}}
            <a {{action showContact this href=true}}>{{unbound displayName}}</a>
          </td>
          <td>
            {{#if user}}
              {{avatarImg user}}
              <a {{action showUser user href=true}}>{{unbound user.displayName}}</a>
            {{/if}}
          </td>
          <td>3 days</td>
          <td>Email Quote Tomorrow</td>
          <td>
            <a href="#">
              <i class="icon-trash"></i>
            </a>
          </td>
        </tr>
      {{/with}}
    {{/each}}
  </tbody>
</table>
