<div class="tabbar">
  <div class="tabbar-inner">
    <ul class="nav nav-tabs">
      {{#each categories}}
        <li>
          {{#link-to "addressbook.filter" name}}
            {{#if isRadium}}<i class="custom-icon-radium"></i> {{/if}}
            {{text}}
          {{/link-to}}
        </li>
      {{/each}}
      <li>
        {{#link-to "externalcontacts"}}
          <i class="custom-icon-google"></i> Untracked Contacts
        {{/link-to}}
      </li>
    </ul>
  </div>
</div>
<div class="page-header">
  <div class="address-book-controls">
    <div class="btn-group">
      <button {{action "confirmDeletion"}} {{bind-attr class=":btn :btn-main :dropdown-toggle controller.hasCheckedContent::muted"}}>
        Delete All
      </button>
    </div>
    <div class="pull-right">
      <form class="form-search pipeline-search">
        <i class="ss-symbolicons-block ss-search"></i>
        {{input value=searchText type="text" class="field search-text"}}
      </form>
      {{#link-to "leads.new" class="btn btn-main btn-primary"}}
        <i class="ss-standard ss-plus"></i> Contact
      {{/link-to}}
    </div>
  </div>
</div>

{{#rd-lazy-each content=model}}
  {{log "Model" this}}
  <div>Hi, my name is {{this.email}}</div>
{{/rd-lazy-each}}
