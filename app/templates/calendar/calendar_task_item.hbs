{{#link-to "calendar.task" item.content.content}}
  <button {{action finishTask}} class="btn btn-link pull-left events-list-item-button">
    {{taskIcon item.content.content}}
  </button>
  <div {{bind-attr class="isFinished :events-list-item-description"}}>
    {{item.title}}
  </div>
{{/link-to}}
