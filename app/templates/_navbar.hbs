<div id="topbar" role="header" class="navbar navbar-inverse navbar-fixed-top">
  <div class="navbar-inner">
    {{#linkTo unimplemented class="brand"}}
      <img src="/images/logo.png" alt="::radium">
    {{/linkTo}}
    <div class="container">
      <ul class="nav">
        <li class="dropdown">
          {{#linkTo messages}}
            Messages <i class="ss-standard ss-mail"></i>
          {{/linkTo}}
        </li>

        <li>
          {{#linkTo calendar today}}
            Calendar <i class="ss-standard ss-calendar"></i>
          {{/linkTo}}
        </li>

        <li>
          {{#linkTo user currentUser}}
            Tasks <i class="ss-standard ss-check"></i>
          {{/linkTo}}
        </li>

        <li>
          {{#linkTo pipeline.index}}
            Pipeline <i class="ss-standard ss-barchart"></i>
          {{/linkTo}}
        </li>

        <li>
          {{#linkTo addressbook.filter 'all'}}
            Address Book <i class="ss-symbolicons-block ss-users"></i>
          {{/linkTo}}
        </li>
      </ul>

      <ul class="nav pull-right">
        <li>
          <a href="https://radium.flowdock.com/invitations/6d70fab64e2ff9222f15f60adb2e8d875ed72c61-support" title="Support">Support</a>
        </li>
      </ul>

      <form class="navbar-search pull-right">
        <input type="text" class="search-query" placeholder="Unimplemented">
      </form>
    </div>
  </div>
</div>