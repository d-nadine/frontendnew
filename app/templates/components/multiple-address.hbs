<div id="locationField">
  <input class="single" id="autocomplete" placeholder="Enter address or city" type="text" autocomplete="off" class="">
</div>
<div id="addressField">
  {{input class="single route" type="text" placeholder="Street"}}<br/>
  {{input class="single postal_town" type="text" placeholder="City"}}<br/>
  {{input class="single administrative_area_level_1" type="text" placeholder="State / Province"}}<br/>
  {{input class="single postal_code" type="text" placeholder="Zip / Postal Code"}}<br/>
  <input class="single" id="country" type="text" placeholder="Country"><br>
  <div id="addressType">
    <div>
      <input type="radio" checked=""><span>Business</span>
    </div>
    <div>
      <input type="radio"><span>Home</span>
    </div>
  </div>
</div>