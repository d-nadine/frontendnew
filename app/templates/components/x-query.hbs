<a href="#" class="delete" {{action "removeQuery" query}}>
  <i class="ss-standard ss-delete"></i>
</a>
<span>{{query.displayName}}</span>
{{#x-select value=query.operator action="changeOperator" class="operator showing"}}
  {{#each operator in operatorSelection}}
    {{#x-option value=operator.value}}{{operator.text}}{{/x-option}}
  {{/each}}
{{/x-select}}
{{view queryValue}}
