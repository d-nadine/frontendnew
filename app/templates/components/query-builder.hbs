<div class="variables">
  {{#each selectedQuery in potentialQueries}}
    {{x-query query=selectedQuery parent=this index=_view.contentIndex}}
  {{/each}}
</div>
<div class="combobox-container">
  {{autocomplete-textbox source=fields value=value class="field" queryKey="displayName" placeholder="Filter By" actionOnly=true action="addPotentialQuery"}}
</div>