<div class="alert">
  <button type="button" class="close" data-dismiss="alert">Ã—</button>
  <strong>Warning!</strong> <span class="name-variable">This person </span> has already been created. You are now editing an existing contact.
</div>
<div class="item contact-name">
  <div class="icon">
    <i class="ss-standard ss-user"></i>
  </div>
  <div class="description">Name</div>
  <div class="field">
    {{autocomplete-textbox scopes="contact" value=model.name queryKey="displayName" bindQueryKey="targetObject.model.name" modelChanged="modelSelected"}}
  </div>
</div>
<div class="item company">
  <div class="icon">
    <i class="ss-standard ss-buildings"></i>
  </div>
  <div class="description">Company</div>
  <div class="field">
    {{autocomplete-textbox scopes="company" value=model.companyName queryKey="name" bindQueryKey="targetObject.model.companyName"}}
  </div>
</div>
